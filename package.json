{
  "name": "@bane-labs/bridge-sdk-ts",
  "version": "1.0.0-beta.6",
  "description": "A library for interacting with the Neo Bridge from TypeScript projects.",
  "license": "Apache-2.0",
  "author": "Otniel Nicola",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "test": "tsx --test test/*.test.ts",
    "test:watch": "node --test --watch test/*.test.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@cityofzion/neon-js": "5.8.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
